{
  "name": "express-best-practice",
  "version": "1.0.0",
  "description": "Node.js Express application with best practices",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "NODE_ENV=test dotenv jest --forceExit --detectOpenHandles",
    "migrate": "node -r dotenv/config ./node_modules/.bin/sequelize-cli db:migrate",
    "seed": "node -r dotenv/config ./node_modules/.bin/sequelize-cli db:seed:all"
  },
  "keywords": [
    "express",
    "nodejs",
    "best-practice"
  ],
  "author": "",
  "license": "ISC",
  "_moduleAliases": {
    "@": "src",
    "@/api": "src/api",
    "@/config": "src/config",
    "@/models": "src/models",
    "@/services": "src/services",
    "@/utils": "src/utils",
    "@/middlewares": "src/api/middlewares",
    "@/validations": "src/api/validators"
  },
  "dependencies": {
    "amqplib": "^0.10.3",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.3.1",
    "express-recaptcha": "^5.1.0",
    "helmet": "^6.0.1",
    "http-status": "^1.6.2",
    "ioredis": "^5.7.0",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "puppeteer": "^24.17.0",
    "rate-limit-redis": "^4.2.0",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "socket.io": "^4.8.1",
    "winston": "^3.8.2",
    "worker-farm": "^1.7.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "dotenv-cli": "^10.0.0",
    "jest": "^30.0.5",
    "jest-cli": "^30.0.5",
    "nock": "^14.0.10",
    "nodemon": "^2.0.20",
    "socket.io-client": "^4.8.1",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.4"
  }
}
